#ifndef _KLIB_CSTRING
#define _KLIB_CSTRING

#include <string.h>

namespace std {
    using ::memset;
    using ::memcpy;
    using ::memmove;
    using ::memcmp;

    using ::strlen;
    using ::strnlen;

    using ::strcpy;
    using ::strncpy;

    using ::strcmp;
    using ::strncmp;

    using ::strchr;

    using ::strrchr;

    using ::strerror;
}

#endif /* _KLIB_CSTRING */